                </div>
                <div class="d-flex justify-content-around">
                    <div>
                        <label class="label drk">Total:</label>
                        <input type="number" value="" placeholder="0" class="input w-4 w-50 prvPregTotal" />
                    </div>
                    <div>
                        <label class="label drk">No Of Living Children:</label>
                        <input type="number" value="" placeholder="0" class="input w-4 w-50 prvPregLivingChild" />
                    </div>
                    <div>
                        <input type="button" value="Add Row" class="btn prvPregAddRowBtn" />
                    </div>
                </div>
            </section>

            <section>
                <div class="v-scroll" style="height: 22rem">
                    <table class="">
                        <thead>
                            <tr>
                                <th class="alc">S/N</th>
                                <th class="px-5 alc">Month/Year_Of_Birth</th>
                                <th class="px-5 alc">Duration_Of_Pregnancy</th>
                                <th class="px-5 alc">Birth_Wt</th>
                                <th class="alc">Sex</th>
                                <th class="px-5 alc">Place_Of_Birth</th>
                                <th class="px-5 alc">Preg_Labor/Puerperium</th>
                                <th class="px-5 alc">Age_At_Death</th>
                                <th class="px-5 alc">Cause_Of_Death</th>
                                <th class="px-5 alc"></th>
                            </tr>
                        </thead>
                        <tbody class="prvPregTbody"></tbody>
                    </table>
                </div>
            </section>
        </div>

        <div class="djq-body3 deep-g d-flex justify-content-around py-3">
            <button type="button" class="btn clm prvPregDeleteBtn">Delete</button>
            <button type="button" class="btn clm prvPregSubmitBtn">Submit</button>
            <button type="button" class="btn clm close-dataJQ prvPregCloseBtn">Close</button>
        </div>

        <input type="hidden" value="" class="prevPregRecno" />

    </div>
</div>



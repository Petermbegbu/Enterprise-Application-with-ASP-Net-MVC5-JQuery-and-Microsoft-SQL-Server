
@{
    ViewBag.Title = "ChangeStatusPvt_Corp";
    ViewBag.TitleBar = "CHANGE PATIENTS/CORPORATE CLIENT STATUS";
    Layout = "~/Views/Shared/_MRFormLayout.cshtml";
}


<section>
    <form method="post" action="#">

        <div class="dif">



            <section class="bdr-f w100">

                <div class="newline n1 ">
                    <label class="label w-5 w-35">Patient Group Code</label>
                    <input type="text" list="cbo" class="input w-4 w-50 increase-height" />
                    <datalist id="cbo">
                        <option value="">-Select VALUE-</option>
                        <option value="VALUE">VALUE</option>
                    </datalist>
                    <input type="button" name="btn" value="" class="btn-small" />
                </div>

                <div class="newline n1 ">
                    <label class="label w-5 w-35">Patient No.</label>
                    <input type="text" list="cbo" class="input w-4 w-50 increase-height" />
                    <datalist id="cbo">
                        <option value="">-Select VALUE-</option>
                        <option value="VALUE">VALUE</option>
                    </datalist>
                    <input type="button" name="btn" value="" class="btn-small" />
                </div>

                <div class="newline n1 ">
                    <label class="label w-5 w-35">Name</label>
                    <input type="text" list="cbo" class="input w-8 w-50 increase-height" />
                    <datalist id="cbo">
                        <option value="">-Select VALUE-</option>
                        <option value="VALUE">VALUE</option>
                    </datalist>
                </div>

                <div class="newline n1 ">
                    <label class="label w-5 w-35">Address</label>
                    <textarea name="txt" value="" rows="3" class="txtarea w-8 w-50"></textarea>
                </div>

                <div class="newline n1 ">
                    <label class="label w-5 w-35">BILLS PAYABLE BY</label>
                    <input type="text" name="txt" value="" class="input w-10 w-50" />
                </div>
            </section>


            <div class="bdr-f">
                <label class="label w-5 w-35">CORPORATE CLIENT</label>
                <input type="text" name="txt" value="" class="input w-4 w-50" />
                <input type="button" name="btn" value="" class="btn-small" />

                <span class="span">
                    <input type="text" name="txt" value="" class="input w-9 w-50" />
                </span>
            </div>


            <section class="bdr-f">

                <div class="newline n1">
                    <label class="label w-5 w-35">Current Status</label>
                    <input type="text" name="txt" value="" class="input w-5 w-50" />
                </div>

                <div class="newline n1">
                    <label class="label w-5 w-35">Select New Status</label>
                    <input type="text" list="cbo" class="input w-6 w-50 increase-height" />
                    <datalist id="cbo">
                        <option value="">-Select VALUE-</option>
                        <option value="VALUE">VALUE</option>
                    </datalist>
                </div>

            </section>

            <div class="newline n1 alc">

                <label class="label w-5 w-35"></label>
                <input type="button" name="btnPrint" value="Submit" class="btn clm" />

                <label class="label w-8 w-35"></label>
                <input type="button" name="btnPrint" value="Close" class="btn clm" />

            </div>



        </div>

    </form>
</section>

